<if><get name='AGENT:enabled' />
<then>
<form action='disable'>
  <input type='hidden' name='disable' value='true' />
  <input type='submit' value='DISABLE' />
</form>
</then><else>
<form action='disable'>
  <input type='hidden' name='enable' value='true' />
  <input type='submit' value='ENABLE' />
</form>
</else></if>
