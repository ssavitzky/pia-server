<!DOCTYPE AGENTS SYSTEM Admin-agent>
<!-- $Id: AGENTS.xml,v 1.4 1999-06-15 16:35:22 steve Exp $ -->

<AGENTS>
  <!-- Note: it is not yet feasible to save a modified version of this file -->
  <!--       using any of the forms in the /Admin agent.                    -->

  <AGENT name='DOFS' path='/' type='DOFS' tagset='pia-xhtml'
	 class='org.risource.pia.agent.Dofs' pathName='/DOFS'>
    <bind name='criteria'></bind>
    <bind name='onStart'>on</bind>
    <bind name='doNotRemove'>LOCKED</bind>
    <bind name='doNotSave'>LOCKED</bind>
  </AGENT>

  <AGENT name='Doc' path='/' type='/DOFS' tagset='pia-xhtml'
	  class='org.risource.pia.agent.Dofs' pathName='/Doc'>
    <bind name='criteria'></bind>
    <bind name='root'>../Doc</bind>
    <bind name='onStart'>on</bind>
    <bind name='doNotRemove'>LOCKED</bind>
    <bind name='doNotSave'>LOCKED</bind>
  </AGENT>

  <AGENT name='PIA' path='/' type='/DOFS' tagset='pia-xhtml'
	  class='org.risource.pia.agent.Dofs' pathName='/PIA'>
    <bind name='criteria'></bind>
    <bind name='root'>../</bind>
    <bind name='onStart'>on</bind>
    <bind name='doNotRemove'>LOCKED</bind>
    <bind name='doNotSave'>LOCKED</bind>
  </AGENT>
</AGENTS>
