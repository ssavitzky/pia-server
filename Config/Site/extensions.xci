<!-- $Id: extensions.xci,v 1.3 1999-10-04 17:29:39 steve Exp $ -->

  <Ext name="html"	type="text/html"				/>
  <Ext name="htm"	type="text/html"				/>

  <Ext name="txt"	type="text/plain"				/>

  <Ext name="xh" 	type="text/html"	tagset="pia-xhtml"	/>

  <Ext name="png"	type="image/png"				/>
  <Ext name="jpeg"	type="image/jpeg"				/>
  <Ext name="jpg"	type="image/jpeg"				/>
  <Ext name="xbm"	type="image/x-xbitmap"				/>
  <Ext name="gif"	type="image/gif"				/>

  <!-- Note that we do not flag .gz files as compressed: we assume that the
       user will decompress them explicitly, rather than letting the browser
       do it behind his back. 
    -->
  <Ext name="exe"	type="application/octet-stream"			/>
  <Ext name="tgz"	type="application/octet-stream"			/>
  <Ext name="tar"	type="application/octet-stream"			/>
  <Ext name="gz"	type="application/octet-stream"			/>

  <!-- Note that the order of extensions in this file becomes the order of
       the search list when an extension is missing.  To keep an extension
       off the list, start the tagset attribute with "#".
    -->
